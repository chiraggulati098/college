   "source": [
    "list1 = ['a',2,3]\n",
    "list2 = [4,'hello',6]\n",
    "list3 = list1 + list2 \n",
    "print(list3)\n",
    "\n",
    "for x in list2:\n",
    "    list1.append(x)\n",
    "print(list1)\n",
    "\n",
    "list1 = ['a',2,3]\n",
    "list1.extend(list2)\n",
    "print(list1)"
   ]
